<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".actividades.CrearFoodtruckActivity" android:background="#ffffff">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="395dp"
            android:layout_height="715dp" tools:layout_editor_absoluteY="8dp" tools:layout_editor_absoluteX="8dp">
        <TextView
                android:text="Nombre"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView20"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/txt_nombre_persona"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Apellido"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView21"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/txt_apellido_foodtruck"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Cédula"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView22"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/txt_cedula_foodtruck"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Fecha de Nacimiento"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView23"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="date"
                android:ems="10"
                android:id="@+id/txt_fecha_foodtruck"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Nombre foodtruck"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView11"
                android:layout_gravity="center|center_horizontal|center_vertical" android:layout_marginTop="14dp"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/txt_nombre_foodtruck" android:layout_gravity="center" android:layout_marginTop="14dp"/>
        <TextView
                android:text="Usuario"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView24"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/txt_usuario_foodtruck"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Contraseña"
                android:layout_width="300dp"
                android:layout_height="35dp" android:id="@+id/textView25"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/txt_contrasenia_foodtruck"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <Button
                android:text="Crear"
                android:layout_width="200dp"
                android:layout_height="wrap_content" android:id="@+id/btn_crear_foodtruck"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>