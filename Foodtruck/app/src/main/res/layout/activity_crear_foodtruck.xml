<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".actividades.CrearFoodtruckActivity" android:background="#ffffff">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="395dp"
            android:layout_height="715dp" tools:layout_editor_absoluteY="8dp" tools:layout_editor_absoluteX="8dp">
        <TextView
                android:text="Nombre"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView20"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText5"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Apellido"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView21"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText6"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Cédula"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView22"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editText8"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Fecha de Nacimiento"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView23"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="10"
                android:id="@+id/editText12"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Usuario"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView24"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <EditText
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText13"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
        <TextView
                android:text="Contraseña"
                android:layout_width="300dp"
                android:layout_height="wrap_content" android:id="@+id/textView25"
                android:layout_gravity="center"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/alegreya_sc"
                android:textSize="18sp" android:textColor="#000000"/>
